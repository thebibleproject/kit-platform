{
    "name": "kit-platform",
    "version": "0.1.0",
    "scripts": {
        "build": "nx run-many --target=build --all",
        "start": "nx run-many --target=serve --projects=client,services-auth,services-content,services-progress",
        "start:progress": "nx run-many --target=serve --projects=client,services-progress",
        "start:gateway": "nx run gateway:serve",
        "lint": "nx run-many --target=lint --all",
        "test": "nx run-many --target=test --all --output-style=static --detectOpenHandles",
        "test:coverage": "nx run-many --target=test --all --output-style=static  --codeCoverage=true --coverageReporters=text,html"
    },
    "private": true,
    "devDependencies": {
        "@apollo/rover": "0.18.1",
        "@nrwl/eslint-plugin-nx": "15.7.2",
        "@nrwl/express": "15.7.2",
        "@nrwl/jest": "15.7.2",
        "@nrwl/js": "15.7.2",
        "@nrwl/linter": "15.7.2",
        "@nrwl/node": "15.7.2",
        "@nrwl/vite": "^15.7.2",
        "@nrwl/webpack": "15.7.2",
        "@nrwl/workspace": "15.7.2",
        "@types/express": "4.17.13",
        "@types/jest": "28.1.1",
        "@types/node": "18.7.1",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "5.36.1",
        "@typescript-eslint/parser": "5.36.1",
        "@vitejs/plugin-react": "^4.1.1",
        "eslint": "8.15.0",
        "eslint-config-prettier": "8.1.0",
        "jest": "28.1.1",
        "jest-environment-jsdom": "28.1.1",
        "nx": "15.7.2",
        "prettier": "2.8.1",
        "ts-jest": "28.0.5",
        "ts-node": "10.9.1",
        "typescript": "4.8.2",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "dependencies": {
        "@apollo/client": "^3.8.7",
        "@apollo/gateway": "2.3.0",
        "@apollo/server": "4.3.0",
        "@apollo/subgraph": "2.0.3",
        "@phosphor-icons/react": "^2.0.14",
        "axios": "1.0.0",
        "express": "4.18.1",
        "graphql": "16.6.0",
        "graphql-depth-limit": "1.1.0",
        "graphql-tag": "2.12.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
